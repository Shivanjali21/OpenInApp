<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.UIDash" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
         <item name="colorPrimary">@color/colorNavbar</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher_foreground</item>
        <item name="postSplashScreenTheme">@style/Theme.AppOpener</item>
        <item name="windowSplashScreenBackground">@color/colorWhite</item>
        <item name="android:windowSplashScreenAnimationDuration" tools:targetApi="s">1000</item>
    </style>

</resources>